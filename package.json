{
	"name": "dracula-theme-refined-no-italics",
	"version": "1.0.0",
	"displayName": "Dracula Refined (without italics)",
	"homepage": "https://github.com/pedrocmota/dracula-theme-refined-no-italics",
	"description": "Yet another Dracula Official Theme fork, but better (in my opinion) and without italics",
	"publisher": "pedrocmota",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/dracula.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"maintainers": [
		"Derek P Sifford <dereksifford@gmail.com>",
		"Matheus Calegaro <matheuscalegaro@outlook.com>"
	],
	"contributors": [
		"Krzysztof Wisznarewski",
		"Daniel Ramos",
		"Eric Jackson",
		"Elliott Pogue",
		"Ajit Singh",
		"Teddy Bradford",
		"Pedro Mota"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/pedrocmota/dracula-theme-refined-no-italics.git"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dracula",
		"dracula-refined",
		"without italics",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "icon.png",
	"contributes": {
		"themes": [
			{
				"label": "Dracula Refined",
				"uiTheme": "vs-dark",
				"path": "./theme/dracula.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^3.14.0",
		"prettier": "^2.2.1",
		"tinycolor2": "^1.4.2",
		"vsce": "^1.85.0"
	}
}
